<!DOCTYPE html>
  <html lang=en>
  <head>
    <meta charset=UTF-8>
    <meta name=viewport content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no">
    <script>
      (function () {
        var baseFontSize = 100;
        var baseWidth = 375;

        var set = function () {
          var clientWidth = document.documentElement.clientWidth || window.innerWidth;

          var rem = 100;
          if (clientWidth != baseWidth) {
            rem = Math.floor(clientWidth / baseWidth * baseFontSize);
          }

          document.querySelector('html').style.fontSize = rem + 'px';
        }
        set();

        window.addEventListener('resize', set);
      }());
    </script>
    <title>京东来点</title>
    <link href=//${staticPathUrl}/nsng/smart-now-app/${staticVersion}/css/app.css rel=stylesheet>
  </head>
  <body>
    <div id=app></div>
    <script type=text/javascript src=//${staticPathUrl}/nsng/smart-now-app/${staticVersion}/js/manifest.js></script>
    <script type=text/javascript src=//${staticPathUrl}/nsng/smart-now-app/${staticVersion}/js/vendor.js></script>
    <script type=text/javascript src=//${staticPathUrl}/nsng/smart-now-app/${staticVersion}/js/app.js></script>
  </body>
  </html>
